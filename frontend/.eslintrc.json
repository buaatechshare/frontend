{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "parser": "babel-eslint"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "vue/no-parsing-error": [
            2,
            {
                "x-invalid-end-tag": false
            }
        ],
        "linebreak-style": [
            0,
            "error",
            "windows"
        ]
    }
}